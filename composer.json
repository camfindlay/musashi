{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:camfindlay/silverstripe-socialnetwork-links.git"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^3.3",
		"silverstripe/framework": "^3.3",
		"silverstripe/reports": "^3.3",
		"silverstripe/siteconfig": "^3.3",
		"silverstripe-themes/simple": "3.1.*",
        "silverstripe/subsites": "^1.2",
        "unclecheese/betterbuttons": "^1.3",
        "silverstripe/userforms": "^3.1",
		"silverstripe/blog": "^2.4",
		"gdmedia/silverstripe-socialnetwork-links": "dev-config-yaml",
		"adrexia/brand": "3.0.x-dev as 1.0.1",
		"silverstripe/widgets": "^1.2",
		"studiobonito/silverstripe-google-analytics": "^2.0",
		"adrexia/subsite-modeladmins": "^1.3",
		"camfindlay/silverstripe-twofactorauth": "^2.1",
		"i-lateral/silverstripe-gallery": "1.3.2"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
